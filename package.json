{
  "name": "ddnet",
  "version": "0.3.5",
  "description": "ddnet.org API wrapper",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.20.2"
  },
  "scripts": {
    "build:src": "pnpm cleanup:dist && tsc --project tsconfig.json",
    "build:docs": "pnpm cleanup:docs && typedoc --plugin typedoc-material-theme --themeColor '#ffa500'",
    "cleanup:dist": "rimraf dist",
    "cleanup:docs": "rimraf docs",
    "prettier": "prettier . --write",
    "dev": "pnpm cleanup && tsc && node --enable-source-maps ."
  },
  "keywords": [
    "ddnet",
    "ddrace",
    "network",
    "teeworlds",
    "ddracenetwork",
    "ddnetwork",
    "teedata"
  ],
  "type": "module",
  "author": "thatboisans",
  "license": "ISC",
  "homepage": "https://sans3108.github.io/DDNet/",
  "bugs": {
    "url": "https://github.com/Sans3108/DDNet/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sans3108/DDNet.git"
  },
  "funding": "https://ko-fi.com/sansy",
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@types/node": "20.10.0",
    "prettier": "3.2.5",
    "rimraf": "^5.0.5",
    "typedoc": "^0.25.13",
    "typedoc-material-theme": "^1.0.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "keyv": "^4.5.4",
    "zod": "^3.23.4"
  },
  "types": "dist/index.d.ts"
}
